{
  "manifest_version": 3,
  "name": "GA Realtime Users",
  "version": "1.2",
  "description": "Shows realtime user data from Google Analytics",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "identity",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Loading...",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "oauth2": {
    "client_id": "594899345250-06vnmiddlb5nq0udoc0g6dsnnpqaqniq.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/analytics.readonly",
      "https://www.googleapis.com/auth/userinfo.email"
    ]
  }
}