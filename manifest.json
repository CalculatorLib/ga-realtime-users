{
  "manifest_version": 3,
  "name": "GA Realtime Users",
  "version": "1.0",
  "description": "Shows realtime user data from Google Analytics",
  "permissions": [
    "identity",
    "storage",
    "tabs"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Loading..."
  },
  "background": {
    "service_worker": "background.js"
  },
  "oauth2": {
    "client_id": "594899345250-06vnmiddlb5nq0udoc0g6dsnnpqaqniq.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/analytics.readonly",
      "https://www.googleapis.com/auth/userinfo.email"
    ]
  }
}